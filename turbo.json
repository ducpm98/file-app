{
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "command": {
        "client": "npm run build --workspace=client"
      }
    },
    "dev": {
      "command": {
        "server": "npm run dev --workspace=server",
        "client": "npm run dev --workspace=client"
      }
    }
  }
}
